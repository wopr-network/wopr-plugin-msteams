{
  "author": "TSavo",
  "dependencies": {
    "@wopr-network/plugin-types": "github:wopr-network/wopr-plugin-types",
    "axios": "^1.13.5",
    "botbuilder": "^4.22.0",
    "winston": "^3.11.0"
  },
  "description": "Microsoft Teams plugin for WOPR using Azure Bot Framework",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@types/node": "^25.3.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "wopr",
    "wopr-plugin",
    "microsoft-teams",
    "teams",
    "azure-bot",
    "bot-framework"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "name": "@wopr-network/wopr-plugin-msteams",
  "scripts": {
    "build": "tsc",
    "check": "biome check src/ && tsc --noEmit",
    "format": "biome format --write src/",
    "lint": "biome check src/",
    "lint:fix": "biome check --fix src/",
    "prepublishOnly": "npm run build",
    "test": "vitest run"
  },
  "types": "dist/index.d.ts",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "extends": "@wopr-network/semantic-release-config"
  }
}
